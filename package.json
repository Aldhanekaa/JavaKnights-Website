{
  "name": "javaknights-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev-tailwind": "concurrently \"next dev\" \"tailwindcss --input ./src/app/globals.css --output ./src/app/output.css --watch\"",
    "dev": "next dev",
    "tailwind": "tailwindcss --input ./src/app/globals.css --output ./src/app/output.css --watch",
